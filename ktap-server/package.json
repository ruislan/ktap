{
    "name": "ktap-server",
    "version": "1.2.0",
    "main": "server.js",
    "author": "Rui",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "NODE_ENV=dev node server.js",
        "dev:seed": "npx prisma db push && npx prisma db seed -- --cmd dev && node server.js",
        "start": "NODE_ENV=production node server.js",
        "seed:base": "npx prisma db seed -- --cmd base",
        "seed:clean": "npx prisma db push --force-reset",
        "seed:dev": "npx prisma db seed -- --cmd dev",
        "seed:steam": "npx prisma db push --force-reset & npx prisma db seed -- --cmd steam"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.1",
        "@fastify/cors": "^10.0.1",
        "@fastify/helmet": "^12.0.1",
        "@fastify/jwt": "^9.0.1",
        "@fastify/multipart": "^9.0.1",
        "@fastify/sensible": "^6.0.1",
        "@fastify/static": "^8.0.2",
        "@node-rs/jieba": "^1.10.4",
        "@prisma/client": "^5.21.1",
        "ajv-errors": "^3.0.0",
        "bcrypt": "^5.1.1",
        "cheerio": "1.0.0-rc.12",
        "dotenv": "^16.4.5",
        "fastify": "^5.0.0",
        "ms": "^2.1.3",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.9.15",
        "pino-pretty": "^11.3.0",
        "sanitize-html": "^2.13.1",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "chokidar": "^4.0.1",
        "prisma": "^5.21.1"
    }
}
