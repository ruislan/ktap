{
    "name": "ktap-server",
    "version": "1.2.0",
    "main": "server.js",
    "author": "Rui",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "NODE_ENV=dev node server.js",
        "dev:seed": "npx prisma db push && npx prisma db seed -- --cmd dev && node server.js",
        "start": "NODE_ENV=production node server.js",
        "seed:base": "npx prisma db seed -- --cmd base",
        "seed:clean": "npx prisma db push --force-reset",
        "seed:dev": "npx prisma db seed -- --cmd dev",
        "seed:steam": "npx prisma db push --force-reset & npx prisma db seed -- --cmd steam"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "dependencies": {
        "@fastify/cookie": "^9.3.1",
        "@fastify/cors": "^8.5.0",
        "@fastify/helmet": "^11.1.1",
        "@fastify/jwt": "^7.2.4",
        "@fastify/multipart": "^8.2.0",
        "@fastify/sensible": "^5.5.0",
        "@fastify/static": "^6.12.0",
        "@node-rs/jieba": "^1.10.0",
        "@prisma/client": "^5.12.1",
        "ajv-errors": "^3.0.0",
        "bcrypt": "^5.1.1",
        "cheerio": "1.0.0-rc.12",
        "dotenv": "^16.4.5",
        "fastify": "^4.26.2",
        "ms": "^2.1.3",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.9.13",
        "pino-pretty": "^9.4.1",
        "sanitize-html": "^2.13.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "chokidar": "^3.6.0",
        "prisma": "^5.12.1"
    }
}
